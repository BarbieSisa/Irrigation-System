<Menus::TopBarComponent @onLogout={{action 'logout'}} @onGoto={{action 'goto'}}/>

<Menus::SideBarComponent @onGoto={{action 'goto'}}>
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 main-content">
        <div style="display: flex">
            {{#unless (index-of this.activeRouteTracker.activeRouteName 'home.index')}}
                <button type="button" class="mr-20 btn btn-primary" style="flex-grow: 0.3;" {{action 'goBack'}}>Back</button>
            {{/unless}}
            <div id="wormhole-container" style="flex-grow: 9.7;"></div>
        </div>
        {{outlet}}
    </main>
</Menus::SideBarComponent>
